        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_prices AS (\n  SELECT DISTINCT ON (symbol, exchange)\n    symbol,\n    exchange,\n    price,\n    timestamp\n  FROM price_history\n  ORDER BY symbol, exchange, timestamp DESC\n)\nSELECT\n  symbol,\n  (MAX(price) - MIN(price)) / MIN(price) * 100 as spread_pct,\n  MIN(price) as min_price,\n  MAX(price) as max_price\nFROM latest_prices\nGROUP BY symbol\nHAVING (MAX(price) - MIN(price)) / MIN(price) * 100 > 0.001\nORDER BY spread_pct DESC",
          "refId": "A"
        }
      ],
      "title": "Top Arbitrage Opportunities (Calculated)",
      "type": "table"
